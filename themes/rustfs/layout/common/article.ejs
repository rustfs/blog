<article class="repo-list">
    <div class="repo-list-item" >
      <h3 class="repo-list-name" itemprop="name">
        <%- partial('post/title', {class_name: 'article-title'}) %>
      </h3>
      <div class="repo-list-description">
        <% if (post.excerpt && index){ %>
          <%- post.excerpt %>
        <% } else { %>
          <%- post.content %>
        <% } %>
      </div>
      <p class="repo-list-meta">
        <span class="meta-info">
          <span class="octicon octicon-calendar"></span>
           <%- partial('post/date', {class_name: 'article-date', date_format: null}) %>
        </span>

        <%if(post.categories && post.categories.length){%>
          <span class="meta-info">
            <span class="octicon octicon-file-directory"></span>
            <%- getCategory(post)%>
          </span>
        <%}%>
        <span class="meta-info read-more">
            <% if (post.link){ %>
                <a  href="<%- url_for(post.link) %>" target="_blank" itemprop="url">阅读更多...</a>
           <% } else if (post.title){ %>
             <% if (index){ %>
                 <a href="<%- url_for(post.path) %>">阅读更多...</a>
             <% } else { %>
                 <%= post.title %>
             <% } %>
           <% } %>
        </span> 
        <!--
          <% if (post.tags && post.tags.length){ %>
            <%- list_tags(post.tags, {
            show_count: false,
            class: 'article-tag'
            }) %>
          <% } %>
        -->

      </p>
    </div>
  </article>